{
    "compilerOptions": {
      "target": "ESNext",
      "module": "commonjs",
      "resolveJsonModule": true,
      "noEmit": true,
      "outDir": "dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "strictPropertyInitialization": false
    }
  }